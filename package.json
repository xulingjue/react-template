{
  "name": "react-template",
  "version": "1.0.1-beta.1",
  "description": "A template for react + redux + webpack",
  "engines": {
    "node": ">=4.5.0",
    "npm": "^3.0.0"
  },
  "scripts": {
    "start": "npm run build:dev:dll && cross-env NODE_ENV=development node server/index.js",
    "start:prod": "cross-env NODE_ENV=production BUILD_ENV=production node server/index.js",
    "build:dev:dll": "cross-env NODE_ENV=development BUILD_ENV=development node build/build-dll.js",
    "build:prod:dll": "cross-env NODE_ENV=production BUILD_ENV=production node build/build-dll.js",
    "build:prod": "npm run build:prod:dll && cross-env NODE_ENV=production BUILD_ENV=production node build/build.js"
  },
  "license": "MIT",
